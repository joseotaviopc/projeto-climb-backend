{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "semi": "off"
      }
    ],
    // "semi": "error",
    "space-before-function-paren": ["off", "always"],
    "comma-dangle": "off",
    "quote": "off"
    // "no-unused-vars":"error"
  }
}
